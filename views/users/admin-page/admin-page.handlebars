 {{!-- {{#each users}}
    <h1>{{this.username}}</h1>
 {{/each}}
 <h1>{{this.username}}</h1> --}}
 
 <div class="container">
     <div class="row">
<table class="table table-hover col-lg-10 usersTable">
  <thead>
 <tbody>
      {{#each users}}
    <tr class="table-active">
       
      <th scope="row">Username:  {{this.username}}</th>
      <td>First Name:  {{this.firstName}}</td>
      <td>Last Name:  {{this.firstName}}</td>
     {{#if roles}}
      <td>Priority:  [ {{this.roles}} ] </td>
      {{else}}
      <td>Priority:  [ none ] </td>

     {{/if}}
      {{/each}}
    </tr>
    </tbody>
 </thead>
 </table>
 </div>
</div>


